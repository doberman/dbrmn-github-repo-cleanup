{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "comma-dangle": 0,
    "space-unary-ops": 0,
    "no-param-reassign": ["error", { "props": false }],
    "semi": 0,
    "no-console": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "space-infix-ops": 0,
    "arrow-body-style": 0
  }
}
